RewriteEngine On

# Redirigir calculadora.html a /calculadora-mercado-objetivo
RewriteRule ^calculadora-mercado-objetivo$ /calculadora.html [L]


# Redirigir URLs sin extensi√≥n a su archivo correspondiente

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^/]+)/?$ $1.html [L]

RewriteRule ^404$ /404.html [L]
ErrorDocument 404 /404
